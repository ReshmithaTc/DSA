class Solution {
    public int maxProfit(int[] prices) {
        int n = prices.length;
        if(n==0) return 0;

        int minprice=prices[0];
        int maxprofit=0;

        for(int i=1; i<n; i++){
            int profitToday=prices[i]-minprice;
            if(profitToday>maxprofit){
                maxprofit=profitToday;
            }
            if(prices[i]<minprice){
                minprice=prices[i];
            }
       }
       return maxprofit;
    }
}
